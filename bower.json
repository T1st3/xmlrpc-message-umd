{
  "name": "xmlrpc-message-umd",
  "version": "0.7.8",
  "homepage": "http://www.tiste.org/xmlrpc-message-umd",
  "description": "A XMLRPC message builder written with UMD definition",
  "keywords": [
    "umd",
    "xmlrpc",
    "builder",
    "xml"
  ],
  "license": "MIT",
  "authors": [
    "T1st3 <contact@tiste.org> (http://www.tiste.org/)"
  ],
  "main": "dist/xmlrpc-message-umd.js",
  "ignore": [
    ".directory",
    ".DS_Store",
    ".git",
    ".gitignore",
    ".gitattributes",
    ".mailmap",
    "node_modules",
    "package.json",
    ".npmignore",
    "bower_components",
    ".bowerrc",
    ".editorconfig",
    ".jshintrc",
    ".jscs.json",
    ".istanbul.yml",
    ".travis.yml",
    "src",
    "test",
    "gh-pages",
    "docs",
    "gulpfile.js",
    "README.md"
  ],
  "dependencies": {
    "requirejs": "~2.1",
    "btoa-umd": "~0.6"
  },
  "devDependencies": {
    "jquery": "~2.1",
    "mocha": "~2.0",
    "chai": "~1.10",
    "chai-jquery": "~2.0",
    "t1st3-assets": "~1.3",
    "codemirror": "~4.7",
    "lodash": "~2.4"
  },
  "overrides": {
    "requirejs": {
      "main": "require.js"
    },
    "jquery": {
      "main": ["dist/jquery.min.js", "dist/jquery.min.map"]
    },
    "bootstrap": {
      "main": ["dist/js/bootstrap.min.js", "dist/css/bootstrap.min.css", "dist/fonts/*"]
    },
    "font-awesome": {
      "main": ["css/font-awesome.min.css", "fonts/*"]
    },
    "modernizr": {
      "main": "modernizr.js"
    },
    "codemirror": {
      "main": ["lib/codemirror.js", "/mode/javascript/javascript.js", "lib/codemirror.css"]
    },
    "respond": {
      "main": "dest/respond.min.js"
    },
    "json3": {
      "main": "lib/json3.min.js"
    },
    "mocha": {
      "main": ["mocha.js", "mocha.css"]
    },
    "chai": {
      "main": "chai.js"
    },
    "chai-jquery": {
      "main": "chai-jquery.js"
    },
    "jshint": {
      "main": "dist/jshint.js"
    },
    "lodash": {
      "main": "dist/lodash.min.js"
    },
    "tist3-assets": {
      "main": ""
    }
  }
}
