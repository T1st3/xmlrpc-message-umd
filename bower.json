{
  "name": "xmlrpc-message-umd",
  "version": "0.8.6",
  "homepage": "http://www.tiste.org/xmlrpc-message-umd",
  "description": "A XMLRPC message builder written with UMD definition",
  "keywords": [
    "xmlrpc",
    "builder",
    "xml",
    "umd"
  ],
  "license": "MIT",
  "authors": [
    "T1st3 <contact@tiste.org> (http://www.tiste.org/)"
  ],
  "main": [
    "./dist/xmlrpc-message-umd.js",
    "./dist/xmlrpc-message-umd.min.js",
    "./dist/xmlrpc-message-umd.js.map"
  ],
  "ignore": [
    ".directory",
    ".DS_Store",
    ".git",
    ".gitignore",
    ".gitattributes",
    ".mailmap",
    "node_modules",
    "package.json",
    ".npmignore",
    "bower_components",
    ".bowerrc",
    ".editorconfig",
    ".jshintrc",
    ".jscs.json",
    ".istanbul.yml",
    ".travis.yml",
    "src",
    "test",
    "gh-pages",
    "docs",
    "gulpfile.js",
    "README.md"
  ],
  "dependencies": {
    "btoa-umd": "~0.7",
    "requirejs": "~2.1"
  },
  "devDependencies": {
    "jquery": "~2.1",
    "bootstrap": "~3.3",
    "font-awesome": "~4.3",
    "modernizr": "~2.8",
    "codemirror": "~5.0",
    "respond": "~1.4",
    "json3": "~3.3",
    "mocha": "~2.2",
    "chai": "~2.0",
    "chai-jquery": "~2.0",
    "jshint": "~2.6",
    "lodash": "~3.5"
  },
  "overrides": {
    "btoa-umd": {
      "main": "dist/btoa-umd.min.js"
    },
    "requirejs": {
      "main": "require.js"
    },
    "modernizr": {
      "main": "modernizr.js"
    },
    "respond": {
      "main": "dest/respond.min.js"
    },
    "json3": {
      "main": "lib/json3.min.js"
    },
    "jquery": {
      "main": [
        "dist/jquery.min.js",
        "dist/jquery.min.map"
      ]
    },
    "lodash": {
      "main": "lodash.min.js"
    },
    "bootstrap": {
      "main": [
        "dist/js/bootstrap.min.js",
        "dist/css/bootstrap.min.css",
        "dist/fonts/*"
      ]
    },
    "font-awesome": {
      "main": [
        "css/font-awesome.min.css",
        "fonts/*"
      ]
    },
    "mocha": {
      "main": [
        "mocha.js",
        "mocha.css"
      ]
    },
    "chai": {
      "main": "chai.js"
    },
    "chai-jquery": {
      "main": "chai-jquery.js"
    },
    "jshint": {
      "main": "dist/jshint.js"
    },
    "codemirror": {
      "main": [
        "lib/codemirror.js",
        "mode/**/*.js",
        "lib/codemirror.css"
      ]
    }
  }
}
